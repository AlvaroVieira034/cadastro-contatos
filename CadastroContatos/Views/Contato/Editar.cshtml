@{
    ViewData["Title"] = "Alteração de Contatos";
}

<div class="text-center">
    <h1 class="display-6">Alteração de Contatos</h1>

</div>

<form>
    <div class="form-group">
        <label for="nome">Nome do Contato</label>
        <input type="text" class="form-control" placeholder="Informe o nome do contato">
    </div>
    <br />

    <div class="form-group">
        <label for="email">e-mail do Contato</label>
        <input type="text" class="form-control" placeholder="Informe o e-mail do contato">
    </div>
    <br />

    <div class="form-group">
        <label for="celular">Celular do Contato</label>
        <input type="text" class="form-control" placeholder="Informe o celular do contato">
    </div>
    <br />

    <button type="submit" class="btn btn-primary">Gravar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>